  "main": "index.js",
  "scripts": {
    "build": "yarn flow && yarn run clean && TARGET=prod NODE_ENV=production yarn run webpack",
    "clean": "rm -rf dist .tmp",
    "flow": "flow",
    "lint": "eslint --cache -c .eslintrc.yml *.js src",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start:local": "TARGET=local yarn run webpack:dev",
    "start:prod": "TARGET=prod yarn run webpack:dev",
    "start": "TARGET=local-staging yarn run webpack:dev",
    "test:watch": "yarn test -- --watch",
    "test": "jest --config test/jest.config.json",
    "webpack:dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "webpack:dev:dashboard": "webpack-dashboard -p 3002 -- webpack:dev",
    "webpack": "webpack --config webpack.config.js"
  },
